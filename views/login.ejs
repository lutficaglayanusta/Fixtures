<%- include("partials/_header") %> <%- include("partials/_navigation") %>
<!-- header section end -->
<!-- contact section start -->
<div class="contact_section layout_padding">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h1 class="contact_taital contact_heading">Log In</h1>
      </div>
    </div>
    <div class="contact_section_2">
      <div class="row">
        <div class="col-md-12">
          <div class="mail_section map_form_container">
            <form action="/users/signin" method="POST">
              <input
                type="email"
                class="mail_text"
                placeholder="Email"
                name="email"
              />
              <input
                type="password"
                class="mail_text"
                placeholder="Password"
                name="password"
              />
              <div class="btn_main">
                <button
                  style="
                    background-color: #f0b016;
                    color: white;
                    border-radius: 20px;
                    margin-top: 10px;
                    padding: 10px 5px;
                    font-weight: bold;
                    font-size: 19px;
                  "
                  type="submit"
                >
                  Send
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- contact section end -->
<!-- footer section start -->
<%- include("partials/_footer") %>
